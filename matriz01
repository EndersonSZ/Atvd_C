#include <stdio.h>


int main()
{

    int matr[3][5];
    int l, c, soma=0;


    for(l = 0; l<3; l++){
        for(c = 0; c<5; c++){

                printf("informe os valores: ", l, c);
                scanf("%d",&matr[l][c]);

        }
    }
     printf("\n");
     for(l = 0; l<3; l++){
        for(c = 0; c<5; c++){

            soma += matr[l][c];
            //imprimindo a matriz na tela
            printf("%d", matr[l][c]);

        }
            printf("\n");
    }


     printf("\n\n  soma total: %d\n \n ",soma);
     printf("\n");

    for(l = 0; l<3; l++){
        for(c = 0; c<5; c++){

            if(matr[l][c] > 100){
                printf("elementos maiores que 100:", matr[l][c]);
            }else{
                printf("nenhum elemento Ã© maior que 100!");
            }

        }
    }

    return 0;
}
